{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "appzcoder/lumen-routes-list": "^1.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "blocktrail/cryptojs-aes-php": "^0.1.0",
        "dusterio/lumen-passport": "^0.2.10",
        "illuminate/mail": "^5.8",
        "laravel/lumen-framework": "5.8.*",
        "league/flysystem": "^1.0",
        "league/fractal": "^0.18.0",
        "mpociot/laravel-apidoc-generator": "^3.9",
        "ramsey/uuid": "^3.8",
        "spatie/laravel-permission": "^2.37",
        "urameshibr/lumen-form-request": "^1.5",
        "vlucas/phpdotenv": "^3.3",
        "vluzrmos/tinker": "^1.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
